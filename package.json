    "vite-plugin-mock-dev-server": "^1.8.4",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-sitemap": "^0.7.1",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.6",
    "vue-tsc": "^2.2.2"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "glob": "7.2.3",
      "inflight": "1.0.6",
