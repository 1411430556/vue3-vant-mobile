    "axios": "^1.7.8",
    "echarts": "^5.5.1",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.6",
    "pinia-plugin-persistedstate": "^4.1.3",
    "resize-detector": "^0.3.0",
    "vant": "^4.9.9",
    "vconsole": "^3.15.1",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.10.0",
    "@iconify-json/carbon": "^1.2.4",
