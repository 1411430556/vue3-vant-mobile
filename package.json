    "@vitejs/plugin-vue": "^5.2.0",
    "autoprefixer": "^10.4.20",
    "bumpp": "^9.8.1",
    "consola": "^3.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.15.0",
    "eslint-plugin-format": "^0.1.2",
    "husky": "^9.1.7",
    "less": "^4.2.1",
    "mockjs": "^1.1.0",
    "postcss-mobile-forever": "^4.3.0",
    "rollup": "^4.27.4",
