    "axios": "^1.7.9",
    "echarts": "^5.6.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "resize-detector": "^0.3.0",
    "vant": "^4.9.17",
    "vconsole": "^3.15.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.3.0",
    "@iconify-json/carbon": "^1.2.7",
